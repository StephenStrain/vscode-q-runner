{
    "name": "q-runner",
    "displayName": "q-runner",
    "description": "",
    "version": "0.0.1",
    "publisher": "jundu",
    "engines": {
        "vscode": "^1.19.0"
    },
    "activationEvents": [
        "onCommand:q-runner.createTerminal",
        "onCommand:q-runner.sendText"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "q-runner.createTerminal",
                "title": "Q: Create RunQ Terminal"
            },
            {
                "command": "q-runner.sendText",
                "title": "Q: Run Selected Text"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
